package components 

templ NoMobile() {
	@Slide() {
		<div class="flex flex-col items-center justify-center h-screen p-6">
			@Title() {
				Hold Up.
			}
			<div class="HelpText text-xl italic text-gray-300 text-center">
				<span>This screen looks way better on a laptop, trust me.</span>
			</div>
		</div>
	}
}
