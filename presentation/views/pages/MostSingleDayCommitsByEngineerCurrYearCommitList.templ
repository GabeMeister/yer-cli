package presentation_views_pages

import (
	"GabeMeister/yer-cli/analyzer"
	components "GabeMeister/yer-cli/presentation/views/components"
)

templ MostSingleDayCommitsByEngineerCurrYearCommitList(recap analyzer.Recap) {
	@components.Slide() {
		<ul class="mt-6 text-2xl h-[500px] w-[805px] overflow-y-scroll">
			for _, commit := range recap.MostSingleDayCommitsByEngineerCurrYear.Commits {
				<li class="mt-3 font-mono text-wrap">"{ commit }"</li>
			}
		</ul>
	}
}
