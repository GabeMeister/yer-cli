package pages

import components "GabeMeister/yer-cli/presentation/views/components"

templ TheEnd() {
	@components.Slide() {
		<div class="flex flex-col items-center justify-center gap-24 p-12 h-screen">
			@components.Title() {
				The End
			}
			<div class="absolute bottom-6">
				@components.ButtonLink("/") {
					<span>Restart</span>
					<i class="ml-1 fa-solid fa-rotate-left"></i>
				}
			</div>
		</div>
	}
}
